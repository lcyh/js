{
    "compilerOptions": {
      "outDir": "dist",
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "module": "esnext",
      "baseUrl": ".",
      "sourceMap": true,
      "moduleResolution": "node",
      "strict": true,
      "noUnusedLocals": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "removeComments": false,
      "rootDir": ".",
      "isolatedModules": true,
    //   "noEmit": true,
      "types": [],
    }
  }
  